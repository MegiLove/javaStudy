class DecimalOctalHexaBinary 
{
	public static void main(String[] args) 
	{
		int a= 12;//10진법
		int b= 014;//8진법
		int c= 0xC;//16진법
		int d= 0b1100;//2진법
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
	}
}

