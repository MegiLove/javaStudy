public class Customer {
    public int calcPrice(int price) {
        bonusPoint += price * bonusRatio;
        return price;
    }
}
public class VIPCustomer extends Customer {
    public int calcPrice(int price) {
        bonusPoint += price * bonusRatio;
        return price - (int)(price * salesRatio);
    }
}

